{
  "name": "docsfinder",
  "displayName": "Docs Finder",
  "description": "",
  "version": "0.0.1",
  "publisher": "sunxen",
  "icon": "logo.png",
  "repository": {
      "type": "git",
      "url": "https://github.com/sunxen/docsfinder"
  },
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "docsfinder.open",
        "title": "Docs Finder"
      }
    ],
    "keybindings":[
      {
        "command": "docsfinder.open",
        "key": "ctrl+shift+k",
        "mac": "cmd+shift+k"
      }
    ]
  },
  "scripts": {
    "build": "vsce package --yarn --baseImagesUrl https://raw.githubusercontent.com/sunxen/docsfinder/main/",
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.82.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "cheerio": "1.0.0-rc.12"
  }
}
